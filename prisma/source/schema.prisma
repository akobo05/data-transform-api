generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/source-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_SOURCE")
}

model RawData {
  id           String   @id @default(uuid())
  text_content String
  created_at   DateTime @default(now())
}
