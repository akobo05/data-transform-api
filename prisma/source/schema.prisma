generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/source-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_SOURCE")
}

model Paciente {
  id                String @id @default(uuid())
  pac_cod           String
  pac_primer_nombre String
  pac_apellido      String
  pac_fec_nac       String
  pac_genero        String
  pac_dx            String
  pac_dx_desc       String
  pac_estado        String
  pac_edad          Int
  fec_registro      String
}
