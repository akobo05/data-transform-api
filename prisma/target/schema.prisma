generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/target-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_TARGET")
}

model ClinicalRecord {
  id                    String   @id @default(uuid())
  person_source_value   String
  given_name            String
  family_name           String
  birth_date            String
  gender                String
  condition_code        String
  condition_description String
  visit_status          String
  age_at_visit          Int
  migrated_at           DateTime @default(now())
}
